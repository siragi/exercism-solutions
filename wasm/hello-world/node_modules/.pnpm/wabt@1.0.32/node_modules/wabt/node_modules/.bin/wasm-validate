#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/simon/Dokumente/gitlocal/exercism/wasm/hello-world/node_modules/.pnpm/wabt@1.0.32/node_modules/wabt/bin/node_modules:/home/simon/Dokumente/gitlocal/exercism/wasm/hello-world/node_modules/.pnpm/wabt@1.0.32/node_modules/wabt/node_modules:/home/simon/Dokumente/gitlocal/exercism/wasm/hello-world/node_modules/.pnpm/wabt@1.0.32/node_modules:/home/simon/Dokumente/gitlocal/exercism/wasm/hello-world/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/simon/Dokumente/gitlocal/exercism/wasm/hello-world/node_modules/.pnpm/wabt@1.0.32/node_modules/wabt/bin/node_modules:/home/simon/Dokumente/gitlocal/exercism/wasm/hello-world/node_modules/.pnpm/wabt@1.0.32/node_modules/wabt/node_modules:/home/simon/Dokumente/gitlocal/exercism/wasm/hello-world/node_modules/.pnpm/wabt@1.0.32/node_modules:/home/simon/Dokumente/gitlocal/exercism/wasm/hello-world/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/wasm-validate" "$@"
else
  exec node  "$basedir/../../bin/wasm-validate" "$@"
fi
