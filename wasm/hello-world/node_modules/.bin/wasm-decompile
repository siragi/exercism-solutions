#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/simon/Dokumente/gitlocal/exercism/wasm/hello-world/node_modules/wabt/bin/node_modules:/home/simon/Dokumente/gitlocal/exercism/wasm/hello-world/node_modules/wabt/node_modules:/home/simon/Dokumente/gitlocal/exercism/wasm/hello-world/node_modules:/home/simon/Dokumente/gitlocal/exercism/wasm/node_modules:/home/simon/Dokumente/gitlocal/exercism/node_modules:/home/simon/Dokumente/gitlocal/node_modules:/home/simon/Dokumente/node_modules:/home/simon/node_modules:/home/node_modules:/node_modules:/home/simon/Dokumente/gitlocal/exercism/wasm/hello-world/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/simon/Dokumente/gitlocal/exercism/wasm/hello-world/node_modules/wabt/bin/node_modules:/home/simon/Dokumente/gitlocal/exercism/wasm/hello-world/node_modules/wabt/node_modules:/home/simon/Dokumente/gitlocal/exercism/wasm/hello-world/node_modules:/home/simon/Dokumente/gitlocal/exercism/wasm/node_modules:/home/simon/Dokumente/gitlocal/exercism/node_modules:/home/simon/Dokumente/gitlocal/node_modules:/home/simon/Dokumente/node_modules:/home/simon/node_modules:/home/node_modules:/node_modules:/home/simon/Dokumente/gitlocal/exercism/wasm/hello-world/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../wabt/bin/wasm-decompile" "$@"
else
  exec node  "$basedir/../wabt/bin/wasm-decompile" "$@"
fi
